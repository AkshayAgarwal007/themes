---
layout: null
---
<?xml version="1.0" encoding="utf-8"?>
<feed xmlns="http://www.w3.org/2005/Atom">
  <title>Jekyll Theme of the Month | {{ site.title }}</title>
  <link href="{{ site.url }}/themes/atom.xml" rel="self"/>
  <link href="{{ site.url }}"/>
  <updated>{{ site.time | date_to_xmlschema }}</updated>
  <id>{{ site.url }}</id>
  <author>
    <name>Planet Jekyll Team</name>
  </author>
  <generator>Jekyll v{{ jekyll.version }}</generator>

  {% for theme in site.themes limit: 10 %}
  <entry>
    <title>{{ theme.title | xml_escape }}</title>
    <link href="{{ site.url }}{{ theme.url }}"/>
    <updated>{{ theme.date | date_to_xmlschema }}</updated>
    <id>{{ site.url }}{{ theme.url }}</id>
    <content type="html">
      {{ theme.content | xml_escape }}
    </content>
  </entry>
  {% endfor %}
</feed>
